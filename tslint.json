{
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "rules": {
    "member-access": [false],
    "ordered-imports": [false],
    "member-ordering": [false],
    "interface-name": [false],
    "arrow-parens": false,
    "object-literal-sort-keys": false
  },
  "linterOptions": {
    "exclude": ["node_modules"]
  }
}
